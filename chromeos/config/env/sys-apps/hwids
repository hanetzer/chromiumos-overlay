# Mask compressed database files as everything uses the uncompressed copies.
hwids_mask="
  /usr/share/misc/pci.ids.gz
  /usr/share/misc/usb.ids.gz
"

PKG_INSTALL_MASK+=" ${hwids_mask}"
INSTALL_MASK+=" ${hwids_mask}"
