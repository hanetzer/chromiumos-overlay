+	&pvrDRI2FlushExtension.base,
+	&pvrDRIImage.base,
+	&pvrDRIRobustness.base,
+#if defined(__DRI_PRIORITY)
+	&pvrDRIPriority.base,
+#endif
+#if defined(__DRI2_FENCE)
+	&pvrDRIFenceExtension.base,
+#endif
+#if defined(__DRI_QUERY_BUFFERS)
+	&pvrDRIQueryBuffers.base,
+#endif
