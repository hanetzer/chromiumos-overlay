+const PVRDRIImageFormat *PVRDRIFormatToImageFormat(int iDRIFormat)
+{
+	unsigned i;
+
+	assert(iDRIFormat != __DRI_IMAGE_FORMAT_NONE);
+
+	for (i = 0; i < ARRAY_SIZE(g_asFormats); i++)
+	{
+		if (g_asFormats[i].iDRIFormat == iDRIFormat)
+		{
+			return &g_asFormats[i];
+		}
+	}
+
+	return NULL;
+}
