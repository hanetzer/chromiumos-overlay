+
+	PVRDRIDrawableDeinit(psPVRDrawable);
+
+	PVREGLDrawableDestroyConfig(psPVRDrawable->psImpl);
+
+	PVRQDequeue(&psPVRDrawable->sQElem);
+
+	PVRMutexDeinit(&psPVRDrawable->sMutex);
+
+	PVRDRIDestroyDrawableImpl(psPVRDrawable->psImpl);
+	free(psPVRDrawable);
+}
