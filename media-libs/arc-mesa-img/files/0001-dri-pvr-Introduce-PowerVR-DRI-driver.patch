+/* We assume the DRI context flags match their EGL counterparts */
+#if __DRI_CTX_FLAG_DEBUG != EGL_CONTEXT_OPENGL_DEBUG_BIT_KHR || \
+    __DRI_CTX_FLAG_FORWARD_COMPATIBLE != EGL_CONTEXT_OPENGL_FORWARD_COMPATIBLE_BIT_KHR || \
+   __DRI_CTX_FLAG_ROBUST_BUFFER_ACCESS != EGL_CONTEXT_OPENGL_ROBUST_ACCESS_BIT_KHR
