+void __attribute__((format(printf, 1, 2))) __driUtilMessage(const char *f, ...)
+{
+	va_list args;
+	char *ev;
+
+#ifdef HAVE_ANDROID_PLATFORM
+	va_start(args, f);
+	dbg_vprintf(f, args);
+	va_end(args);
+#else
+	ev = getenv("LIBGL_DEBUG");
+	if (ev != NULL && (strcmp(ev, "verbose") == 0))
+	{
+		fputs("LibGL: ", stderr);
+		va_start(args, f);
+		dbg_vprintf(f, args);
+		va_end(args);
+		fputc('\n', stderr);
+	}
